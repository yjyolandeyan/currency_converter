{
  "manifest_version": 3,
  "name": "Currency Converter",
  "description": "This extension fetches the currency rate.",
  "version": "1.0",
  "host_permissions": [
    "https://v6.exchangerate-api.com/*"
  ],
   "icons": {
    "16": "images/cc-home-logo-16.png",
    "48": "images/cc-home-logo-48.png",
    "128": "images/cc-home-logo-128.png"
  },
  "action": {
    "default_icon": "images/cc-home-logo.png",
    "default_popup": "CurrencyConverter.html"
  }
}
