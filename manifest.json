{
  "manifest_version": 3,
  "name": "Currency Converter",
  "description": "This extension fetches the currency rate.",
  "version": "1.0",
  "host_permissions": [
    "https://v6.exchangerate-api.com/*"
  ],
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}
